  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^9.0.3 || ^8.0.0",
    "@nestjs/core": "^9.0.3 || ^8.0.0",
    "koishi": "^4.11.0",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@nestjs/platform-express": "^9.0.3",
    "@nestjs/platform-fastify": "^9.0.3",
