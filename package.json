{
  "name": "koishi-nestjs",
  "version": "5.2.3",
  "description": "Koishi.js as Nest.js Module",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --fix .",
    "build": "rimraf dist && tsc",
    "test": "jest --passWithNoTests"
  },
  "repository": {
