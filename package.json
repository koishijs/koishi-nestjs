    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "express": "^4.17.1",
    "jest": "^29.2.2",
    "prettier": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@nestjs/platform-ws": "^9.0.3",
    "@nestjs/websockets": "^9.0.3",
    "@types/ws": "^8.5.3",
